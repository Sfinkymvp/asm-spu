IN            // Ввод коэффициента a
POPR RAX
IN            // Ввод коэффициента b
POPR RBX
IN            // Ввод коэффициента c
POPR RCX
PUSHR RAX
PUSH 0
JNE :QUADRATIC        // Если a != 0, то решается квадратное уравнение, иначе линейное
PUSH -1
PUSHR RCX
MUL
PUSHR RBX
DIV
OUT
PUSH 1
OUT
HLT
:QUADRATIC    // Вычисление дискриминанта D = b^2 - 4ac
PUSHR RBX         
PUSHR RBX
MUL
PUSH 4
PUSHR RAX
PUSHR RCX
MUL
MUL
SUB
POPR RDX 
PUSHR RDX
PUSH 0
JAE :ROOTS_EXISTS        // Если D < 0, то программа завершается
PUSH 0
OUT
HLT
:ROOTS_EXISTS
PUSHR RDX    // Находится дискриминант из неотрицательного дискриминанта
SQRT
POPR RDX
PUSH -1
PUSHR RBX
MUL
PUSHR RDX
ADD
PUSH 2
PUSHR RAX
MUL
DIV
POPR RCX
PUSHR RDX
PUSH 0
JNE :TWO_ROOTS   // Если D = 0, то ищется 1 корень, иначе 2
PUSHR RCX
OUT
PUSH 1
OUT          // Печатается корень и число 1 (количество корней)
HLT
:TWO_ROOTS
PUSH -1 
PUSHR RBX
MUL
PUSHR RDX
SUB
PUSH 2
PUSHR RAX
MUL
DIV
POPR RDX
PUSHR RCX
PUSHR RDX
JB :BELOW    // Корни печатаются в порядке возрастания и число 1 (количество корней)
PUSHR RDX
OUT
PUSHR RCX
OUT     
PUSH 2
OUT
HLT
:BELOW
PUSHR RCX
OUT
PUSHR RDX
OUT
PUSH 2
OUT
HLT
